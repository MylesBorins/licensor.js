{
  "name": "licensor",
  "description": "generate a LICENSE file from package.json",
  "version": "0.0.0-prerelease",
  "author": "Kyle E. Mitchell <kyle@kemitchell.com> (http://kemitchell.com)",
  "bin": "./source/licensor",
  "bugs": "https://github.com/kemitchell/licensor/issues",
  "dependencies": {
    "jslicense-bsd-2-clause": "jslicense/jslicense-bsd-2-clause",
    "jslicense-isc": "jslicense/jslicense-isc",
    "jslicense-mit": "jslicense/jslicense-mit",
    "normalize-package-data": "^2.1.0",
    "spdx": "^0.4.0",
    "word-wrap": "^1.0.3"
  },
  "devDependencies": {
    "fixpack": "^2.2.0",
    "jscs": "^1.13.1",
    "jshint": "^2.7.0",
    "tap": "^1.0.0"
  },
  "homepage": "https://github.com/kemitchell/licensor.js",
  "keywords": [
    "SPDX",
    "law",
    "legal",
    "license",
    "metadata",
    "package"
  ],
  "license": "Apache-2.0",
  "main": "source/licensor.js",
  "repository": "kemitchell/licensor.js",
  "scripts": {
    "lint": "fixpack && jshint source/licenserc.js && jscs source/licenserc.js",
    "precommit": "npm run lint && npm run test",
    "test": "tap test"
  }
}
